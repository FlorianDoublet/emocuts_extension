{
  "name": "Facebook emojis shortcuts",
  "description": "Permit emojis shortcuts to access to emojis into a conversation",
  "version": "0.1",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/*"
      ],
      "js": ["require.js", "shortcut_handler.js", "content.js","jquery-3.2.0.min.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version":2
}