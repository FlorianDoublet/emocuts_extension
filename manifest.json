{
  "name": "Emocuts",
  "description": "Permit d'utiliser le panel d'Ã©mojis de facebook.com au clavier.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
